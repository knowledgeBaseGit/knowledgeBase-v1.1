@charset "utf-8";
/* CSS Document */
body {
	background: url(../images/back.jpg) repeat;
	font-family: "宋体";
	font-size: 12px;
	color: #5f5f5f;
	overflow-x: hidden
}

ul,li {
	list-style: none;
	list-style-type: none;
	padding: 0;
	margin: 0;
}

img {
	border: none
}

.all {
	width: 1000px;
	height: 100%;
	margin: 0 auto;
}

.left {
	float: left;
	width: 25%;
}

.right {
	float: right;
	width: 74%;
	background: #FFFFFF;
}

.lefttop {
	height: 10px;
	border: 1px #e9e9e9 solid;
	background: #FFFFFF
}

.leftmiddle {
	height: 546px;
	background: #FFFFFF;
	border-left: 1px #e9e9e9 solid;
	border-right: 1px #e9e9e9 solid;
	overflow: auto
}

.leftbottom {
	height: 10px;
	border-bottom: 1px #e9e9e9 solid;
	border-top: 1px #e9e9e9 solid;
	background: #FFFFFF;
	border-left: 1px #e9e9e9 solid;
	border-right: 1px #e9e9e9 solid;
}

/*左侧菜单*/
#nav a {
	display: block;
	height: 45px;
	line-height: 45px;
	border-bottom: 1px #e9e9e9 solid;
	font-size: 14px;
	font-weight: bolder;
	color: #5f5f5f;
	text-decoration: none;
	padding-left: 10px;
}

#nav.dyn li ul {
	display: none;
}

#nav.dyn li ul.show {
	display: block;
}

#nav.dyn li ul.show a {
	background: #dad8d9;
	padding-left: 30px;
}

#nav.dyn li.open {
	background: url(../images/Icontree2.jpg) no-repeat 210px 15px;
}

#nav.dyn li.parent {
	background: url(../images/Icontree1.jpg) no-repeat 210px 15px;
}

.righttop {
	height: 50px;
	border: 1px #e9e9e9 solid;
	background: #FFFFFF;
	line-height: 50px;
}

.righttop-left {
	float: left;
	padding-left: 10px;
}

.inputText {
	width: 150px;
	border: 1px #e9e9e9 solid;
}

.inputbutton {
	background: url(../images/BtnChoose.jpg) no-repeat;
	width: 56px;
	height: 20px;
	border: none;
	color: #FFFFFF
}

.righttop-right {
	float: right;
	padding-right: 10px;
	height: 26px;
	padding-top: 12px;
}

.righttop-right ul,.righttop-right li {
	float: left;
	height: 26px;
	line-height: 26px;
}

.righttop-right li a {
	display: block;
	width: 50px;
	height: 26px;
	background: #357a0f;
	margin-left: 5px;
	color: #FFFFFF;
	text-align: center;
	text-decoration: none;
}

.rightmiddle {
	background: #FFFFFF;
	height: 516px;
}

#tab1 {
	width: 100%;
	border: none;
	border-top: 1px #e9e9e9 solid;
}

#tab1 td {
	height: 40px;
	border-bottom: 1px #e9e9e9 solid;
	border-right: 1px #e9e9e9 solid;
	text-align: center
}

.ou {
	background: #f8f8f8
}

.ji {
	background: #FFFFFF
}

#tab1 span {
	float: right;
	padding-right: 10px;
}

.tab2 {
	width: 800px;
}

.tab2 td {
	text-align: center
}
/***********************************/
#tag {
	width: 600px;
	padding-top: 10px;
	padding-left: 15px;
	height: 230px;
	padding-bottom: 10px;
}

.menu0 {
	height: 24px;
}

.menu0 li {
	float: left;
	width: 101px;
	height: 30px;
	line-height: 30px;
	cursor: pointer;
	border: 1px solid #e9e9e9;
	background: #fff;
}

.menu0 li.hover {
	color: #357a0f;
	border-bottom: 1px #FFFFFF solid
}

#tagContent0 ul {
	display: none;
}

#tagContent0 ul.block {
	display: block;
}

#tagContent0 {
	height: 200px;
	width: 580px;
	border: 1px solid #e9e9e9;
	margin-top: 7px;
}

.pop_add {
	display: none;
	width: 500px;
	height: 150px
}

.pop_update {
	display: none;
	width: 500px;
	height: 150px
}

.pop_see {
	display: block;
	width: 700px;
	margin:0 auto;
	height:800px;
}