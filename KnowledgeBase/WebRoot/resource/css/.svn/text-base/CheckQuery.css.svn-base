.tab3{ width:100%; height:100%}
.textareastyle{ width:650px; height:300px; margin-left:10px; margin-top:10px;}
.tab3 span{ padding-right:10px; }


/***********************************/
#tag{ width:700px; padding-top:10px; padding-left:5px; height:300px; padding-bottom:10px;}
.menu0{ height:10px;}
.menu0 li{ float:left; width:101px; height:36px; line-height:36px; text-align:center; cursor:pointer; border:1px solid #e9e9e9; background:#fff;}
.menu0 li.hover{color:#357a0f; border-bottom:1px #FFFFFF solid}
#tagContent0 ul{ display:none;}
#tagContent0 ul.block{ display:block;}
#tagContent0{ height:150px; width:530px; border:1px solid #e9e9e9; margin-top:27px;}

.poptab{ width:100%; height:140px; margin:0 auto;}

/*二层弹出层*/
.white_content {display: none; position:absolute; top:5%; left:40%; width:350px; border:2px #CCCCCC solid; z-index:1002; background:#fff;}
h2{color:#5f5f5f;font-size:18px;height:20px;text-align:right; padding-top:10px; padding-right:10px; cursor:move; margin:0px; border-bottom:1px #CCC solid;}
h2 a{ color:#000; text-decoration:none;}
h2 span{ float:left; padding-left:10px;}

