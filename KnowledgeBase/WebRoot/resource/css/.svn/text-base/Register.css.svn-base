@charset "utf-8";
/* CSS Document */
body{ background:url(../images/back.jpg) repeat; font-family:"宋体"; font-size:12px;}
.all{width:500px; margin:0 auto;padding-top:200px;}
.top{ background:url(../images/topback3.jpg) repeat-x; height:50px;  line-height:50px; color:#FFFFFF; font-family:"黑体"; font-size:30px;padding-left:10px;}
.top-right{ clear:both; float:right;  padding-right:10px; font-family: "宋体"; font-size: 12px; }
.top-right a{color: #ffffff}
.center{ background:#FFFFFF; border:1px #CCCCCC solid;}
.input{ background:#FFFFFF; width:300px; height:19px; border:1px #CCCCCC solid;}
.input1{ background:url(../images/topback3.jpg) repeat-x; width:300px; height:21px; border:none; color:#FFFFFF}
.tab{ margin:0 auto; padding-top:10px; padding-bottom:10px;}
.tab td{ height:30px;}