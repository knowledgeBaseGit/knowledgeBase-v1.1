<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>

	<package name="/files" namespace="/files" extends="json-default">
		<action name="filesManage_*" method="{1}" class="fileManageAction">

		</action>


		<action name="onlineSeeFile_*" method="{1}" class="fileManageAction">
			<result name="success">/index_doc.jsp</result>

		</action>
		
		<action name="onlineSeeClose_*" method="{1}" class="fileManageAction">
			<result name="success">/jsp/File.jsp</result>

		</action>
	</package>
</struts>
 